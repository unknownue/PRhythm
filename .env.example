# Example environment variables file for PRhythm
# Copy this file to .env and fill in your actual values

# GitHub API token (for retrieving PR information)
GITHUB_TOKEN=your_github_token_here

# LLM API key (default LLM provider)
LLM_API_KEY=your_llm_api_key_here

# DeepSeek API key (if using DeepSeek as LLM provider)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Notion API key (if using Notion as publishing platform)
# NOTION_API_KEY=your_notion_api_key_here

# Timezone setting (optional)
TZ=Asia/Shanghai 